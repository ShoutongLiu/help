.center {
    padding: 20rpx 60rpx;
}

.center .avatar {
    margin: 40rpx 0;
}


.center .avatar .img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    display: block;
    margin: 0 auto;
}


.center .info {
    display: flex;
    flex-wrap: wrap;
}

.center .info .item {
    width: 50%;
    margin-top: 20rpx;
}

.login-out {
    margin: 100rpx auto;
    text-align: center;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    background-color: #00BFFF;
    border: 2rpx solid #00BFFF;
    color: #fff;
    border-radius: 40rpx;
    font-size: 32rpx;
}