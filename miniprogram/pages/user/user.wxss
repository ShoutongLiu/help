@import '../../assets/iconfont.wxss';

page {
    background-color: #f0f0f0;
}

.header {
    display: flex;
    padding: 60rpx 40rpx;
    align-items: center;
    background-image: url('data:image/png;base64,');
}

.header .avatar {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}

.header .login-btn {
    width: 200rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    margin-left: 40rpx;
    color: #fff;
    border: 1rpx solid #00BFFF;
    background-color: #00BFFF;
    border-radius: 40rpx;
    font-size: 24rpx;
}

.header .center {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-left: 40rpx;
}

.header .center .user-name {
    font-size: 36rpx;
    font-weight: bold;
}

.header .center .person {
    margin-left: 40rpx;
}

.main {
    margin-top: 20rpx;
    padding: 0 20rpx;
}

.main .order {
    background-color: #fff;
    border-radius: 20rpx;
}

.main .my-order {
    display: flex;
    justify-content: space-between;
    padding: 20rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.main .my-order .order-title {
    font-size: 26rpx;
    font-weight: bold;
}

.main .my-order .all {
    font-size: 24rpx;
    color: #ccc;
}

.main .my-order .icon-right {
    font-size: 24rpx;
}

.main .order .tab {
    display: flex;
    padding: 20rpx 0;
}

.main .tab .tab-item {
    width: 20%;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
}

.main .tab .tab-item .dot {
    position: absolute;
    top: 0;
    right: 10rpx;
    width: 28rpx;
    height: 28rpx;
    line-height: 28rpx;
    background-color: #D81712;
    font-size: 24rpx;
    text-align: center;
    border-radius: 50%;
    padding: 4rpx;
    color: #fff;
}

.main .tab-item .iconfont {
    color: #00BFFF;
    font-size: 48rpx;
}

.main .tab-item .item-text {
    font-size: 24rpx;
    color: #333;
    margin-top: 10rpx;
}

.main .card {
    margin-top: 20rpx;
    background-color: #fff;
    border-radius: 20rpx;
}

.main .card .item-card {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx 20rpx;
    border-bottom: 1rpx solid #f0f0f0;
}